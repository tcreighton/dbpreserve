<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment>The driver name is an older approach.  DriverManager makes this no longer necessary.</comment>
    <entry key="password">postgres</entry>
    <entry key="userName">postgres</entry>
    <entry key="connectionString">jdbc:postgresql://localhost:5432/Chinook</entry>
    <entry key="driverName">com.postgresql.jdbc.Driver</entry>
    <entry key="databaseName">chinook</entry>
    <entry key="schemaName">public</entry>
</properties>
